{
  "name": "mukhda",
  "version": "0.0.0-beta",
  "description": "Contains the frontend for chitrakar which can be used to render photorealistic images",
  "keywords": [
    "raytracing"
  ],
  "author": "Lokesh Raj Arora <lokesh.slg06@gmail.com>",
  "homepage": "https://github.com/lokiiarora/chitrakar#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lokiiarora/chitrakar.git"
  },
  "scripts": {
    "test": "node ./__tests__/mukhda.test.js",
    "clean": "rm -rf dist",
    "clean:bundle": "npm run clean && npm run bundle",
    "bundle": "NODE_ENV=production rollup -c",
    "bundle:alongside": "BUNDLED_BUILD=true npm run bundle",
    "dev": "rollup -c -w",
    "dev:alongside": "BUNDLED_BUILD=true npm run dev"
  },
  "bugs": {
    "url": "https://github.com/lokiiarora/chitrakar/issues"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/three": "^0.143.0"
  },
  "dependencies": {
    "three": "^0.139.2",
    "three-gpu-pathtracer": "^0.0.6"
  }
}
